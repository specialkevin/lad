<table class="table table-hover">
    <thead>
        <tr>
            <th>Link</th>
            <th>Submitted By</th>
        </tr>
    </thead>
    <tbody>
        <% for @link in @links %>
            <tr>
                <td>
                    <a href="<%= @link.url %>"><%= @link.url %></a>
                </td>
                <td>
                    <a href="/user/<%= @link.submitter %>"><%= @link.submitter %></a>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
