<ul class="thumbnails">
    <% for @link in @links %>
        <li class="span4">
            <a href="<%= @link.url %>">
                <img src="<% if @link.photo_id == NIL %>
                    <%= @link.url %>
                    <% else %>
                        <%= "data:image/png;base64," @link.photo %>
                    <% end %>" alt="">
            </a>
        </li>
    <% end %>
</ul>
