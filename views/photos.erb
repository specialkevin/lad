<ul class="thumbnails">
    <% for @link in @links %>
        <li class="span4">
            <a href="<%= @link.url %>">
                <img src="<% if @link.photo.read %>
                    <%= @link.photo.read %>
                    <% else %>
                        <%= @link.url %>
                    <% end %>" alt="">
            </a>
        </li>
    <% end %>
</ul>
